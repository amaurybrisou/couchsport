    "moment": "*",
    "vue": "^2.6.11",
    "vue-i18n": "^8.22.0",
    "vue-native-websocket": "*",
    "vue-router": "^3.4.4",
    "vue2-leaflet": "^2.6.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.5.1",
    "webfontloader": "*"
  },
  "devDependencies": {
    "@vue/cli": "^4.5.9",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
